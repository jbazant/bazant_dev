  <head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <title>{{ (title or eleventyNavigation.title) | noNbsp }} | {{ site.name }}</title>
  <meta name="Description" content="{{ (eleventyNavigation.description or description or metadata.description) | noNbsp }}" />
  <meta property="og:title" content="{{ (title or eleventyNavigation.title) | noNbsp }}" />
  <meta property="og:description" content="{{ (eleventyNavigation.description or description or metadata.description) | noNbsp }}" />
  <meta property="og:image" content="{{ (og_image or '/images/og/bazant-dev.jpeg') | absoluteUrl(metadata.url) }}" />
  <meta property="og:url" content="{{ (page.url or '') | absoluteUrl(metadata.url) }}" />
  <meta property="og:type" content="website" />
  <meta name="twitter:card" content="summary_large_image" />

  {% if page.url %}<link rel="canonical" href="{{ (page.url or '') | absoluteUrl(metadata.url) }}" />{% endif %}
  <link rel="preload" href="{{ '/fonts/entypo.woff' }}" as="font" type="font/woff" crossorigin="anonymous" />
  <link rel="preload" href="{{ '/fonts/entypo.ttf' }}" as="font" type="font/ttf" crossorigin="anonymous" />
  <link rel="stylesheet" href="{{ ('/css/main.css?v=' + cache.cssTimestamp) | url }}" />

  {%- for size in metadata.icons.appleTouchIcon -%}
    <link rel="apple-touch-icon" sizes="{{ size }}" href="{{ '/images/ico/favicon-' + size + '.png' | url }}" />
  {%- endfor -%}
  {%- for size in metadata.icons.favicon -%}
    <link rel="icon" type="image/png" sizes="{{ size }}" href="{{ '/images/ico/favicon-' + size + '.png' | url }}"/>
  {%- endfor -%}
  <link rel="manifest" href="{{ '/manifest.json' | url }}"/>
  <meta name="msapplication-TileColor" content="#ffffff"/>
  <meta name="msapplication-TileImage" content="/favicon-144.png"/>
  <meta name="theme-color" content="#ffffff"/>
</head>
