{
  "include": ["./**/*"],
  "exclude": ["node_modules", "_site", "eleventy"],
  "compilerOptions": {
    "lib": ["webworker", "esnext"],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2016",
    "allowJs": false
  }
}
